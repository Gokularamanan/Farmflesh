<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.farmflesh.app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name">
        <activity
            android:name=".Welcome.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Welcome.GcmRegActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".Welcome.GmailSignInActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".Welcome.MyAccountActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity android:name=".Welcome.WelcomeActivity" />
        <activity
            android:name=".Main.Main2Activity"
            android:label="@string/title_activity_main2"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>